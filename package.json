{
  "name": "refactoring-book",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build-and-start": "rm -rf dist && tsc && node ./dist/index.js",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "dev": "rm -rf dist && tsc -w",
    "prepare": "husky install",
    "start": "node ./dist/index.js",
    "test": "jest",
    "test:only-changed": "jest --onlyChanged",
    "test:only-changed:watch": "jest --onlyChanged --watch",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "source": "src/index.ts",
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@types/jest": "27.5.1",
    "@types/ramda": "0.28.13",
    "babel-jest": "28.1.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "ramda": "0.28.0"
  }
}
